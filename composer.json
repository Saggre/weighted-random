{
  "name": "saggre/weighted-random",
  "description": "Get weighted random samples using the alias method",
  "keywords": [
    "random",
    "faker",
    "alias",
    "weighted",
    "probability",
    "sample"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sakri Koskimies",
      "email": "sakri.koskimies@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Saggre\\WeightedRandom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Saggre\\WeightedRandom\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2"
  }
}
